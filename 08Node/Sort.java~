public class Sort{
    public static void radixsort(MyLinkedListImproved<Integer> data)
    {
	//sudo code
	/*
	  -make "buckets" for data needed
	*/
	MyLinkedListImproved<Integer>[] b = new MyLinkedListImproved<Integer>[10];
	for(int c=0; c<((Integer)data.max()/(Math.pow(10,(Math.log(data)*10/10)))); i++){
	    for(Integer elem : data){
		bucket[(elem / Math.pow(10, i)) % 10].add(elem);
	    }
	    data.clear();
	    for(int i = 0; i < bucket.length; i++){
		data.extend(bucket[i]);
	    }
	}
    }   
    public static void main(String[] args){
     	a.add(0);
	a.add(1);
	a.add(2);
	a.add(3);
	a.add(4);
	MyLinkedListImproved<Integer> b=new MyLinkedListImproved<Integer>();
	b.add(5);
	b.add(6);
	b.add(7);
	b.add(8);
	b.add(9);
	a.extend(b);
	System.out.println(a.toString());
	radixsort(a);
	System.out.println(a.toString());
    }

}
    
